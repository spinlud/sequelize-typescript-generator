{
  "name": "sequelize-typescript-generator",
  "version": "9.0.2",
  "description": "Automatically generates typescript models compatible with sequelize-typescript library (https://www.npmjs.com/package/sequelize-typescript) directly from your source database.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "stg": "build/bin/cli.js"
  },
  "scripts": {
    "clean": "run-script-os",
    "clean:windows": "if exist build rd /s /q build",
    "clean:default": "rm -fr build",
    "lint": "eslint --fix --ext .ts output-models/*.ts",
    "build": "npm run clean && echo Building ... && tsc && echo ... done.",
    "audit": "npm audit",
    "docker-remove-containers": "bash ./src/tests/integration/docker-remove-containers.sh",
    "docker-start-mysql": "bash ./src/tests/integration/mysql/docker-start-mysql.sh",
    "docker-start-mariadb": "bash ./src/tests/integration/mariadb/docker-start-mariadb.sh",
    "docker-start-postgres": "bash ./src/tests/integration/postgres/docker-start-postgres.sh",
    "docker-start-mssql": "bash ./src/tests/integration/mssql/docker-start-mssql.sh",
    "docker-start-all": "npm run docker-remove-containers && npm run docker-start-mysql && npm run docker-start-mariadb && npm run docker-start-postgres && npm run docker-start-mssql",
    "test-mysql": "npm run docker-start-mysql && jest src/tests/integration/mysql",
    "test-mariadb": "npm run docker-start-mariadb && jest src/tests/integration/mariadb",
    "test-mssql": "npm run docker-start-mssql && jest src/tests/integration/mssql",
    "test-mssql:debug": "node --nolazy --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --colors --verbose",
    "test-postgres": "npm run docker-start-postgres && jest src/tests/integration/postgres",
    "test-sqlite": "jest src/tests/integration/sqlite",
    "test": "npm run test-mariadb && npm run test-mssql && npm run test-mysql && npm run test-postgres && npm run test-sqlite",
    "test-all": "npm run docker-start-all && jest src/tests/integration",
    "dev": "npx ts-node-dev src/bin/cli.ts -D mysql -u root -x mysql -d dbtest --dialect-options '{\"timezone\": \"local\"}' --case const:camel --clean --logs",
    "usage": "npx ts-node-dev src/bin/cli.ts --help"
  },
  "author": "Ludovico Fabbri",
  "license": "ISC",
  "repository": "https://github.com/spinlud/sequelize-typescript-generator.git",
  "bugs": {
    "url": "https://github.com/spinlud/sequelize-typescript-generator.git/issues"
  },
  "homepage": "https://github.com/spinlud/sequelize-typescript-generator.git#readme",
  "devDependencies": {
    "@types/bluebird": "^3.5.38",
    "@types/glob": "^8.0.0",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.11.10",
    "@types/pluralize": "0.0.29",
    "@types/validator": "^13.7.10",
    "@types/yargs": "^17.0.15",
    "jest": "^28.1.3",
    "jest-html-reporters": "^3.0.11",
    "mariadb": "^3.0.2",
    "mysql2": "^2.3.3",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "run-script-os": "^1.1.6",
    "sqlite3": "^5.1.2",
    "tedious": "^14.7.0",
    "ts-jest": "^28.0.8",
    "ts-node-dev": "^2.0.0",
    "wkx": "^0.5.0"
  },
  "dependencies": {
    "@types/eslint": "^8.4.10",
    "@typescript-eslint/parser": "^5.45.0",
    "change-case": "^4.1.2",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "pluralize": "^8.0.0",
    "prettier": "2.7.1",
    "sequelize": "^6.26.0",
    "sequelize-typescript": "^2.1.5",
    "typescript": "^4.9.3",
    "yargs": "^17.6.2"
  },
  "peerDependencies": {
    "typescript": "^4.9.3"
  },
  "keywords": [
    "sequelize",
    "sequelize-typescript",
    "sequelize-typescript-generator",
    "sequelize-typescript-auto",
    "knex",
    "bookshelf",
    "typescript",
    "javascript",
    "sql",
    "mysql",
    "postgres",
    "mariadb",
    "mssql",
    "sqlite",
    "orm",
    "node",
    "npm"
  ]
}
